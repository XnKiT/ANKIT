{
    "name": "Telethon Music Bot",
    "description": "Music allow you to stream music trought the telegram voice chat feature Based on Telethon.",
    "logo": "https://te.legra.ph/file/1ee4f0185a4e85cfe689d.jpg",
    "keywords": ["Ankit", "Ankit-Vc-Player", "music", "voice chat", "telegram", "Xnkit", "Xnkit Music", "Private Music", "telethon"],
    "repository": "https://github.com/XNKIT/XNKIT",
  "stack": "container",
    "env": {
        "STRING_SESSION": {
            "description": "fill with the telethon session string.",
            "required": true
        },
        "HEROKU_MODE": {
            "description": "Don't Change this!",
            "required": true,
            "value": "ENABLE"
        },
        "API_ID": {
            "description": "your Api ID from my.telegram.org/apps",
            "required": true,
            "value": "6435225"
        },
        "API_HASH": {
            "description": "your Api Hash from my.telegram.org/apps",
            "required": false,
            "value": "4e984ea35f854762dcde906dce426c2d"
        },
        "SUPPORT": {
            "description": "Fill there your group username else leave",
            "required": false,
            "value": "XnKiTKuMaR"
        },
        "CHANNEL": {
            "description": "Fill there your Channel username else leave",
            "required": false,
            "value": "XnKiTKuMaR"
        },
        "BOT_USERNAME": {
            "description": "Fill there your bot username",
            "required": false,
            "value": ""
        },
        "BOT_TOKEN": {
            "description": "fill with your bot token get from @BotFather on telegram",
            "required": true,
            "value": ""
        }

    }
  }
